<h1>Listing exercise_entries</h1>

<table>
  <tr>
    <th>Date</th>
    <th>Exercise</th>
    <th>Exercise detail</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @exercise_entries.each do |exercise_entry| %>
  <tr>
    <td><%= exercise_entry.date %></td>
    <td><%= exercise_entry.exercise_id %></td>
    <td><%= exercise_entry.exercise_detail_id %></td>
    <td><%= link_to 'Show', exercise_entry %></td>
    <td><%= link_to 'Edit', edit_exercise_entry_path(exercise_entry) %></td>
    <td><%= link_to 'Destroy', exercise_entry, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

